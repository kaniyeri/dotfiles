syntax on
filetype on
filetype indent on
set whichwrap+=<,>,h,l,[,]
set autoindent
map <C-a> gg V G
set nu
set shiftwidth=4
set backspace=indent,eol,start
set clipboard:unnamed
vnoremap <C-c> "*y
map <F9> :!g++ -g % -o %:r && %:r <CR>
filetype plugin indent on
set tabstop=4
set shiftwidth=4
set expandtab

